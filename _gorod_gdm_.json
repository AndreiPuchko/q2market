   "name": "_gorod_gdm__row_extender",
   "script": "def row_extender(data, id):\r\n    for index,x in enumerate(data[\"X\"].replace(\" \", \"\")):\r\n        data[f\"{index+1}X\"] = x    \r\n    for index,x in enumerate(data[\"Y\"].replace(\" \", \"\")):\r\n        data[f\"{index+1}Y\"] = x\r\n    \r\n    prices = []\r\n\r\n    def get_price(key=\"\", key_text=\"\", column=\"\", check_price=0, uncheck_price=0, data=data, price=prices):\r\n        if key_text in data.get(column,\"\"):\r\n            data[key] = \"\u2611\ufe0f\"\r\n            prices.append(check_price)\r\n        else:\r\n            data[key] = \"\u2610\"\r\n            prices.append(uncheck_price)\r\n\r\n\r\n    get_price(\"1EC\", \"Ja\", \"AA\", -5)\r\n    get_price(\"2EC\", \"Ja\", \"AB\", 0, 20)\r\n    get_price(\"3EC\", \"Ja\", \"AC\")\r\n    \r\n    get_price(\"SG180_1\", \"1 Tag\", \"E\", 95)\r\n    get_price(\"SG180_2\", \"2 Tag\", \"E\", 140)\r\n    get_price(\"SG180_3\", \"3 Tag\", \"E\", 210)\r\n    get_price(\"SG60_1\", \"60\", \"E\", 50)\r\n    get_price(\"SG60_2\", \"120\", \"E\", 95)\r\n    \r\n    get_price(\"RP\", \"Russisch f\u00fcr 4-5 Jahre\", \"F\", 60)\r\n    get_price(\"LO\", \"Logik\", \"F\", 10)\r\n    get_price(\"TA\", \"Tanz\", \"F\", 10)\r\n    get_price(\"RU\", \"\u0420\u0443\u0441\u0441\u043a\u0438\u0439 (1-11\", \"F\", 70)\r\n    get_price(\"UK\", \"\u0423\u043a\u0440\u0430\u0407\u043d\u0441\u044c\u043a\u0438\u0439\", \"F\", 70)\r\n    get_price(\"BE\", \"\u0411\u0435\u043b\u0430\u0440\u0443\u0441\u043a\u0438\u0439\", \"F\", 70)\r\n    get_price(\"DE\", \"Deutsch\", \"F\", 70)\r\n    \r\n    get_price(\"LO1\", \"30 Min\", \"AF\", 35)\r\n    get_price(\"LO2\", \"45 Min\", \"AF\", 45)\r\n    get_price(\"LO3\", \"60 Min\", \"AF\", 55)\r\n    \r\n    get_price(\"KU1\", \"Kunst, 2-6 Jahre, \u0420\u0438\u0441\u043e\u0432\u0430\u043d\u0438\u0435 2-6 \u043b\u0435\u0442, 45\", \"H\", 60)\r\n    get_price(\"KU2\", \"Kunst, 2-6 Jahre, \u0420\u0438\u0441\u043e\u0432\u0430\u043d\u0438\u0435 2-6 \u043b\u0435\u0442, 45\", \"H\", 70)\r\n    get_price(\"KU3\", \"Puppentheater\", \"H\", 60)\r\n    get_price(\"KU4\", \"Kinder ab 7 Jahre, 60 Min\", \"H\", 60)\r\n    get_price(\"KU5\", \"Bastelkurs \", \"H\", 50)\r\n    \r\n    \r\n    get_price(\"TA1\", \"30 Min\", \"AE\", 35)\r\n    get_price(\"TA2\", \"45 Min\", \"AE\", 40)\r\n    get_price(\"TA3\", \"60 Min\", \"AE\", 40)\r\n    \r\n    get_price(\"UKT1\", \"70\", \"K\", 70)\r\n    get_price(\"UKT2\", \"45\", \"K\", 45)",
